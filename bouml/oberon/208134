	state 260998 "Query snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "namespace http = boost::beast::http;
#ifndef NDEBUG
BOOST_LOG_TRIVIAL(debug) << \"HTTP GET \" << stm.https_req_.target();
#endif
http::async_write(
    *stm.https_, stm.https_req_,
    boost::asio::bind_executor(
        *stm.strand_, [&stm](const boost::system::error_code &ec,
                          std::size_t bytes_transferred) {
          if (ec) {
            stm.ec_ = ec;
            stm.error_message_ = ec.message();
            stm.failure();
          } else {
            stm.success();
          }
        }));
"
	  transition 500486 "success"
	    on state_ref 261126 // Read snapshot
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	state 261126 "Read snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "boost::beast::http::async_read(
    *stm.https_, stm.https_buffer_, stm.https_res_,
    boost::asio::bind_executor(*stm.strand_,
                               [&stm](const boost::system::error_code &ec,
                                      std::size_t bytes_transferred) {
                                 if (ec) {
                                   stm.ec_ = ec;
                                   stm.error_message_ = ec.message();
                                   BOOST_LOG_TRIVIAL(warning)
                                       << stm.error_message_;
                                   stm.failure();
                                 } else
                                   stm.success();
                               }));
"
	  transition 501510 "success"
	    on state_ref 261382 // Parse snapshot
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	pseudostate 334982 final ""
	end

	state 261382 "Parse snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "boost::asio::post(*stm.ioc_, [&stm]() {
      // BOOST_LOG_TRIVIAL(debug) << stm.https_res_.body();
      std::istringstream is{stm.https_res_.body()};
      stm.https_res_.body().clear();
      stm.snapshot_tree_.clear();
      try {
        read_json(is, stm.snapshot_tree_);
        stm.snapshot_seq_number_ =
            stm.snapshot_tree_.get<long long>(\"sequence\");
#ifdef DEBUG
        BOOST_LOG_TRIVIAL(debug)
            << \"Snapshot sequence number: \" << stm.snapshot_seq_number_;
#endif
        stm.https_buffer_.consume(stm.https_buffer_.size());
        boost::asio::post(
            boost::asio::bind_executor(*stm.strand_, [&stm]() { stm.success(); }));
      } catch (...) {
        boost::asio::post(boost::asio::bind_executor(*stm.strand_, [&stm]() {
          stm.error_message_ = \"Failed to parse the order book snapshot\";
          stm.failure();
        }));
      }
    });
"
	  transition 501638 "success"
	    on pseudostate_ref 334982 // final 
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	transition 501766 "cancel"
	  on state_ref 261510 // Cancelled
	    uml_trigger "cancel"
	    cpp_trigger "cancel"
	  external 
	end

	transition 502278 "<transition>"
	  on pseudostate_ref 335110 // junction 
	    uml_trigger "failure"
	    cpp_trigger "failure"
	  external 
	end
      end

      pseudostate 334598 final ""
      end

