      state 260358 "Obtain"
	  uml_on_entry "create https stream"
	  uml_on_exit "close https stream"
	  cpp_on_entry "stm.snapshot_seq_number_ = 0;
stm.resolver_ = std::make_unique<boost::asio::ip::tcp::resolver>(*stm.ioc_);
stm.https_ctx_ = std::make_unique<boost::asio::ssl::context>(
    boost::asio::ssl::context::sslv23_client);
load_root_certificates(*stm.https_ctx_);
stm.https_ = std::make_unique<SslStream>(*stm.ioc_, *stm.https_ctx_);

if (!SSL_set_tlsext_host_name(stm.https_->native_handle(), stm.https_host_.c_str())) {
  boost::system::error_code ec{static_cast<int>(::ERR_get_error()),
                               boost::asio::error::get_ssl_category()};
  BOOST_LOG_TRIVIAL(error) << ec.message();
}

/*
SSL_CTX_set_keylog_callback(stm.https_ctx_->native_handle(),
      [](const SSL *ssl, const char *line) {
  ofstream f{\"./key_https.txt\", ios_base::app};
  f << line << \"\\n\";
  f.close();
      });
*/

"
	  cpp_on_exit "stm.https_->next_layer().close(stm.ec_);
#ifndef NDEBUG
if(stm.ec_)
 BOOST_LOG_TRIVIAL(warning) << \"Https closing error: \" << stm.ec_.message(); 
#endif
"
	state 260486 "Resolve"
	    uml_on_entry "launch"
	    cpp_on_entry "stm.resolver_->async_resolve(
    stm.https_host_, stm.https_port_,
    boost::asio::bind_executor(
        *stm.strand_,
        [&stm](const boost::system::error_code &ec,
               boost::asio::ip::tcp::resolver::results_type results) {
          if (ec) {
            stm.ec_ = ec;
            stm.error_message_ = ec.message();
            stm.failure();
          } else {
            stm.results_ = results;
            stm.success();
          }
        }));
"
	  transition 500102 "success"
	    on state_ref 260742 // Connect
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	pseudostate 334470 initial ""
	  transition 499590 "<transition>"
	    on state_ref 260486 // Resolve
	    external 
	  end
	end

	transition 499974 "completion"
	  on pseudostate_ref 334598 // final 
	    uml_expr "report success"
	    cpp_expr "boost::asio::post(
    boost::asio::bind_executor(*stm.receiver_->rtc_, [&stm]() {
      stm.receiver_->success();
    }));
"
	  external 
	end

	state 260742 "Connect"
	    uml_on_entry "launch"
	    cpp_on_entry "boost::asio::async_connect(
    stm.https_->next_layer(), stm.results_.begin(), stm.results_.end(),
    boost::asio::bind_executor(
        *stm.strand_, [&stm](const boost::system::error_code &ec,
                          decltype(stm.results_)::iterator iter) {
          if (ec) {
            stm.ec_ = ec;
            stm.error_message_ = ec.message();
            stm.failure();
          } else {
#ifndef NDEBUG
            BOOST_LOG_TRIVIAL(debug) << \"IP: \" << iter->endpoint().address().to_string()
                      << \" Port: \" << iter->endpoint().port() << \"\\n\";
#endif
            stm.https_address_ = iter->endpoint().address();
            stm.success();
          }
        }));
"
	  transition 500230 "success"
	    on state_ref 260870 // Handshake SSL
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	state 260870 "Handshake SSL"
	    uml_on_entry "launch"
	    cpp_on_entry "stm.https_->async_handshake(
    boost::asio::ssl::stream_base::client,
    boost::asio::bind_executor(*stm.strand_,
                               [&stm](const boost::system::error_code &ec) {
                                 if (ec) {
                                   stm.ec_ = ec;
                                   stm.error_message_ = ec.message();
                                   stm.failure();
                                 } else {
                                   stm.success();
                                 }
                               }));
