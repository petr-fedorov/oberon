	      cpp_trigger "success"
	    external 
	  end
	end

	state 260998 "Quering snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "namespace http = boost::beast::http;
#ifndef NDEBUG
BOOST_LOG_TRIVIAL(debug) << \"HTTP GET \" << stm.d_->req_.target();
#endif
auto self(stm.shared_from_this());
http::async_write(
    *stm.d_->https_, stm.d_->req_,
    boost::asio::bind_executor(*stm.d_->strand_,
                               [&stm, self](const boost::system::error_code &ec,
                                            std::size_t bytes_transferred) {
                                 boost::ignore_unused(bytes_transferred);
                                 if (ec) {
                                   stm.d_->ec_ = ec;
                                   stm.d_->error_message_ = ec.message();
                                   stm.failure();
                                 } else {
                                   stm.success();
                                 }
                               }));
"
	  transition 500486 "success"
