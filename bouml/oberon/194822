	state 260870 "Doing SSL handshake"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
stm.d_->https_->async_handshake(
    boost::asio::ssl::stream_base::client,
    boost::asio::bind_executor(*stm.d_->strand_,
                               [&stm, self](const boost::system::error_code &ec) {
                                 if (ec) {
                                   stm.d_->ec_ = ec;
                                   stm.d_->error_message_ = ec.message();
                                   stm.failure();
                                 } else {
                                   stm.success();
                                 }
                               }));
"
	  transition 500358 "success"
