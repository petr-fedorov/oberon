      state 260358 "Obtaining"
	  uml_on_entry "create https stream"
	  uml_on_exit "close https stream"
	  cpp_on_entry "stm.d_->resolver_ = std::make_unique<boost::asio::ip::tcp::resolver>(*stm.d_->ioc_);
stm.d_->ctx_ = std::make_unique<boost::asio::ssl::context>(
    boost::asio::ssl::context::sslv23_client);
load_root_certificates(*stm.d_->ctx_);
stm.d_->https_ = std::make_unique<SnapshotData::SslStream>(*stm.d_->ioc_, *stm.d_->ctx_);

if (!SSL_set_tlsext_host_name(stm.d_->https_->native_handle(), stm.d_->host_.c_str())) {
  boost::system::error_code ec{static_cast<int>(::ERR_get_error()),
                               boost::asio::error::get_ssl_category()};
  BOOST_LOG_TRIVIAL(error) << ec.message();
}

/*
SSL_CTX_set_keylog_callback(stm.d_->ctx_->native_handle(),
      [](const SSL *ssl, const char *line) {
  ofstream f{\"./key_https.txt\", ios_base::app};
  f << line << \"\\n\";
  f.close();
      });
*/

"
	  cpp_on_exit "stm.d_->https_->next_layer().close(stm.d_->ec_);
#ifndef NDEBUG
if(stm.d_->ec_)
 BOOST_LOG_TRIVIAL(warning) << \"Https closing error: \" << stm.d_->ec_.message(); 
#endif
"
	state 260486 "Resolving"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
stm.d_->resolver_->async_resolve(
    stm.d_->host_, stm.d_->port_,
    boost::asio::bind_executor(
        *stm.d_->strand_,
        [&stm, self](const boost::system::error_code &ec,
               boost::asio::ip::tcp::resolver::results_type results) {
          if (ec) {
            stm.d_->ec_ = ec;
            stm.d_->error_message_ = ec.message();
            stm.failure();
          } else {
            stm.d_->results_ = results;
            stm.success();
          }
        }));
"
	  transition 500102 "success"
	    on state_ref 260742 // Connecting
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	pseudostate 334470 initial ""
	  transition 499590 "<transition>"
	    on state_ref 260486 // Resolving
	    external 
	  end
	end

	transition 499974 "completion"
	  on pseudostate_ref 334598 // final 
	    uml_expr "report success"
	    cpp_expr "if(stm.d_->rh_) stm.d_->rh_(std::move(stm.d_->snapshot_tree_));


"
	  external 
	end

	state 260742 "Connecting"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
boost::asio::async_connect(
    stm.d_->https_->next_layer(), stm.d_->results_.begin(), stm.d_->results_.end(),
    boost::asio::bind_executor(
        *stm.d_->strand_, [&stm, self](const boost::system::error_code &ec,
                          decltype(stm.d_->results_)::iterator iter) {
          if (ec) {
            stm.d_->ec_ = ec;
            stm.d_->error_message_ = ec.message();
            stm.failure();
          } else {
#ifndef NDEBUG
            BOOST_LOG_TRIVIAL(debug) << \"IP: \" << iter->endpoint().address().to_string()
                      << \" Port: \" << iter->endpoint().port() << \"\\n\";
#endif
            stm.d_->address_ = iter->endpoint().address();
            stm.success();
          }
        }));
"
	  transition 500230 "success"
	    on state_ref 260870 // Doing SSL handshake
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	state 260870 "Doing SSL handshake"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
stm.d_->https_->async_handshake(
    boost::asio::ssl::stream_base::client,
    boost::asio::bind_executor(*stm.d_->strand_,
                               [&stm, self](const boost::system::error_code &ec) {
                                 if (ec) {
                                   stm.d_->ec_ = ec;
                                   stm.d_->error_message_ = ec.message();
                                   stm.failure();
                                 } else {
                                   stm.success();
                                 }
                               }));
"
	  transition 500358 "success"
	    on state_ref 260998 // Quering snapshot
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	state 260998 "Quering snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "namespace http = boost::beast::http;
#ifndef NDEBUG
BOOST_LOG_TRIVIAL(debug) << \"HTTP GET \" << stm.d_->req_.target();
#endif
auto self(stm.shared_from_this());
http::async_write(
    *stm.d_->https_, stm.d_->req_,
    boost::asio::bind_executor(*stm.d_->strand_,
                               [&stm, self](const boost::system::error_code &ec,
                                            std::size_t bytes_transferred) {
                                 boost::ignore_unused(bytes_transferred);
                                 if (ec) {
                                   stm.d_->ec_ = ec;
                                   stm.d_->error_message_ = ec.message();
                                   stm.failure();
                                 } else {
                                   stm.success();
                                 }
                               }));
"
	  transition 500486 "success"
	    on state_ref 261126 // Reading snapshot
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	state 261126 "Reading snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
boost::beast::http::async_read(
    *stm.d_->https_, stm.d_->buffer_, stm.d_->res_,
    boost::asio::bind_executor(*stm.d_->strand_,
                               [&stm](const boost::system::error_code &ec,
                                      std::size_t bytes_transferred) {
                                 boost::ignore_unused(bytes_transferred);
                                 if (ec) {
                                   stm.d_->ec_ = ec;
                                   stm.d_->error_message_ = ec.message();
                                   stm.failure();
                                 } else
                                   stm.success();
                               }));
"
	  transition 501510 "success"
	    on state_ref 261382 // Parsing snapshot
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	pseudostate 334982 final ""
	end

	state 261382 "Parsing snapshot"
	    uml_on_entry "launch"
	    cpp_on_entry "auto self(stm.shared_from_this());
boost::asio::post(*stm.d_->ioc_, [&stm]() {
      // BOOST_LOG_TRIVIAL(debug) << stm.d_->res_.body();
      std::istringstream is{stm.d_->res_.body()};
      stm.d_->res_.body().clear();
      stm.d_->snapshot_tree_ = std::make_unique<boost::property_tree::ptree>();
      try {
        read_json(is, *stm.d_->snapshot_tree_);
        stm.d_->buffer_.consume(stm.d_->buffer_.size());
        boost::asio::post(
            boost::asio::bind_executor(*stm.d_->strand_, [&stm]() { stm.success(); }));
      } catch (...) {
        boost::asio::post(boost::asio::bind_executor(*stm.d_->strand_, [&stm]() {
          stm.d_->error_message_ = \"Failed to parse the order book snapshot\";
          stm.failure();
        }));
      }
    });
"
	  transition 501638 "success"
	    on pseudostate_ref 334982 // final 
	      uml_trigger "success"
	      cpp_trigger "success"
	    external 
	  end
	end

	transition 501766 "stop"
	  on state_ref 261510 // Stopping
	    uml_trigger "stop"
	    cpp_trigger "stop"
	  external 
	end

	transition 502278 "<transition>"
	  on pseudostate_ref 335110 // junction 
	    uml_trigger "failure"
	    cpp_trigger "failure"
	    cpp_expr "BOOST_LOG_TRIVIAL(warning) << stm._machine_name << stm.d_->error_message_;
"
	  external 
	end
      end

      pseudostate 334598 final ""
      end

