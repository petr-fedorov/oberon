class BitfinexChanged
!!!793094.cpp!!!	BitfinexChanged(in tree : ptree, in reconstructor : BitfinexReconstructor)
timestamp_ = toTimestamp(tree.get<std::string>("exchange_timestamp"));
heard_ = toTimestamp(tree.get<std::string>("local_timestamp"));
order_id_ = toUuid(tree.get<std::string>("order_id"));
price_ = std::stod(tree.get<std::string>("price"));
auto r = std::stod(tree.get<std::string>("amount"));
if (r < 0) {
  remaining(-r);
  side_ = kAsk;
} else {
  remaining(r);
  side_ = kBid;
}
