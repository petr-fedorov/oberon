    state 295302 "Synchronizer"
      stereotype "machine"
      statediagram 453254 "Synchronizer"
	statediagramsettings show_trans_definition yes
	 end
	size A4
      end

      state 295430 "Unsynchronised"
	  uml_on_entry "clear before and era
"
	  cpp_on_entry "stm.before_messages_.clear();
stm.era_messages_.clear();
stm.era_.reset();

"
	transition 557702 "era"
	  on state_ref 295558 // Synchronizing
	    uml_trigger "era"
	    cpp_trigger "era"
	  external 
	end

	transition 557958 "reset"
	  on state_ref 295430 // Unsynchronised
	    uml_trigger "reset"
	    uml_expr "pass reset to next"
	    cpp_trigger "reset"
	    cpp_expr "stm.output_.push_back(std::move(stm.received_));"
	  external 
	end

	transition 558086 "message"
	  on state_ref 295430 // Unsynchronised
	    uml_trigger "message"
	    uml_expr "buffer before"
	    cpp_trigger "message"
	    cpp_expr "stm.received_->deleted(true);
stm.before_messages_.push_back(std::move(stm.received_));
"
	end
      end

      pseudostate 376326 initial ""
	transition 557574 "<transition>"
	  on state_ref 295430 // Unsynchronised
	  external 
	end
      end

      state 295558 "Synchronizing"
	  uml_on_entry "clear era buffer, capture era message"
	  cpp_on_entry "stm.era_ = std::move(stm.received_);
stm.era_messages_.clear();"
	transition 557830 "reset"
	  on state_ref 295430 // Unsynchronised
	    uml_trigger "reset"
	    cpp_trigger "reset"
	  external 
	end

	transition 558214 "era continue"
	  on state_ref 295558 // Synchronizing
	    uml_trigger "message"
	    uml_guard "message.timestamp == era"
	    uml_expr "move to era buffer"
	    cpp_trigger "message"
	    cpp_guard "stm.received_->timestamp() == stm.era_->timestamp()"
	    cpp_expr "stm.era_messages_.push_back(std::move(stm.received_));
"
	end

	transition 558342 "era"
	  on state_ref 295558 // Synchronizing
	    uml_trigger "era"
	    cpp_trigger "era"
	  external 
	end

	transition 558470 "era ended"
	  on state_ref 295686 // Synchronized
	    uml_trigger "message"
	    uml_guard "message.timestamp > era"
	    uml_expr "synchronize buffers before and era"
	    cpp_trigger "message"
	    cpp_guard "stm.received_->timestamp() > stm.era_->timestamp()"
	    cpp_expr "for(auto &msg: stm.era_messages_) {
  auto iter = std::find_if(stm.before_messages_.begin(), stm.before_messages_.end(),
                      [order_id = static_cast<OrderEvent *>(msg.get())->maker()](const Token &b) {
                        auto e = static_cast<OrderEvent*>(b.get());
                        return e->maker() == order_id &&
                               e->state() == OrderEvent::OrderState::kFinished;
                      });
  if(iter != stm.before_messages_.end())
    msg->deleted(true);
}
"
	  external 
	end

	transition 558598 "before era"
	  on state_ref 295558 // Synchronizing
	    uml_trigger "message"
	    uml_guard "message.timestamp < era"
	    uml_expr "move to before buffer"
	    cpp_trigger "message"
	    cpp_expr "stm.received_->deleted(true);
stm.before_messages_.push_back(std::move(stm.received_));
"
	end
      end

      state 295686 "Synchronized"
	  uml_on_entry "move buffers to output"
	  cpp_on_entry "stm.output_.push_back(std::move(stm.era_));
std::move(stm.before_messages_.begin(), stm.before_messages_.end(), std::back_inserter(stm.output_));
std::move(stm.era_messages_.begin(), stm.era_messages_.end(), std::back_inserter(stm.output_));
stm.output_.push_back(std::move(stm.received_));"
	transition 558726 "message"
	  on state_ref 295686 // Synchronized
	    uml_trigger "message"
	    uml_expr "pass to output"
	    cpp_trigger "message"
	    cpp_expr "stm.output_.push_back(std::move(stm.received_));
"
	end

	transition 558854 "reset"
	  on state_ref 295430 // Unsynchronised
	    uml_trigger "reset"
	    uml_expr "pass reset to next"
	    cpp_trigger "reset"
	    cpp_expr "stm.output_.push_back(std::move(stm.received_));"
	  external 
	end

	transition 559110 "era"
	  on state_ref 295558 // Synchronizing
	    uml_trigger "era"
	    cpp_trigger "era"
	  external 
	end
      end
    end

