   BOOST_LOG_TRIVIAL(debug) <<  stm._machine_name << "execute entry behavior of .Coinbase.Engage.Subscribed.Snapshot.Resolve";
#endif
stm.resolver_->async_resolve(
    stm.https_host_, stm.https_port_,
    boost::asio::bind_executor(
        *stm.rtc_,
        [&stm](const boost::system::error_code &ec,
               boost::asio::ip::tcp::resolver::results_type results) {
          if (ec) {
            stm.ec_ = ec;
            stm.error_message_ = ec.message();
