class BitstampReconstructor
!!!759814.cpp!!!	extract(in tree : ptree [1]) : Token [1..*]
using namespace date;
using namespace std;
string type = tree.get<string>("event");
Token output;
if (type == "elapsed") {
  Message::Timestamp timestamp;
  std::istringstream ss{tree.get<string>("timestamp")};
  ss >> parse("%FT%TZ", timestamp);
  output=make_unique<Elapsed>(timestamp);
} else if (type == "order_created")
  output=make_unique<BitstampOpened>(tree, *this);
else if (type == "order_changed")
  output=make_unique<BitstampChanged>(tree, *this);
else if (type == "order_deleted") {
    output=make_unique<BitstampCanceled>(tree, *this);
} else if (type == "trade") {
  output=make_unique<BitstampFilled>(tree, *this);
} else if (type == "era") {
  output=make_unique<BitstampEra>(tree);
}
return output;
