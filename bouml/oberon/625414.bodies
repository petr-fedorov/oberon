class BitfinexCanceled
!!!793478.cpp!!!	BitfinexCanceled(in tree : ptree, in reconstructor : BitfinexReconstructor)
timestamp_ = toTimestamp(tree.get<std::string>("exchange_timestamp"));
heard_ = toTimestamp(tree.get<std::string>("local_timestamp"));
order_id_ = toUuid(tree.get<std::string>("order_id"));
baseIncrement(reconstructor.base_increment_);
if(std::stod(tree.get<std::string>("amount")) < 0) {
  side_ = kAsk;
} else
  side_ = kBid;
